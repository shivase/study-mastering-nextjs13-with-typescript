{
  "name": "boilerplate-nextjs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbo\" \"tailwindcss -i src/app/globals.css -o src/app/output.css -w\"",
    "build": "tailwindcss -m -i src/app/globals.css -o src/app/output.css && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf node_modules .next",
    "prepare": "husky install",
    "preinstall": "typesync || :",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "clsx": "^2.0.0",
    "fast-sort": "^3.4.0",
    "next": "13.5.2",
    "prisma": "^5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-markdown": "^8.0.7",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@storybook/addon-essentials": "^7.4.4",
    "@storybook/addon-interactions": "^7.4.4",
    "@storybook/addon-links": "^7.4.4",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.4",
    "@storybook/builder-webpack5": "^7.4.4",
    "@storybook/nextjs": "^7.4.4",
    "@storybook/react": "^7.4.4",
    "@storybook/testing-library": "^0.2.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "8.44.2",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/node": "^20.6.4",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "cz-customizable": "^7.0.0",
    "daisyui": "^3.7.7",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.14",
    "eslint-plugin-testing-library": "^6.0.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.30",
    "prettier": "3.0",
    "prettier-plugin-tailwindcss": "0.5",
    "react-docgen-typescript-plugin": "^1.0.5",
